{
  "name": "sp-classic",
  "version": "1.0.0",
  "private": true,
  "description": "SharePoint Classic template using React, Flux, PnPjs, TypeScript, Sass, WebPack, Jest (test), Istanbul (code coverage) and PnP PowerShell for deployment. Working on SP Online and 2016. The project also includes a SP authentication proxy. IE 10+ compatible.",
  "main": "index.js",
  "scripts": {
    "test": "jest --c ./config/Jest/jest.config.json --no-cache && node ./config/process-coverage-report.js",
    "build": "webpack --mode production --config ./config/webpack/webpack.config.js --progress --profile",
    "start": "webpack-dev-server --mode development --config ./config/webpack/webpack.dev.server.config.js --display-error-details",
    "clean": "rimraf dist/**/*.* package/010_Style_Library/*.js package/010_Style_Library/*.css package/010_Style_Library/*.map",
    "lint": "tslint -c tslint.json src/**/*.ts src/**/*.tsx",
    "proxy": "node ./api-server.js",
    "serve": "concurrently --kill-others \"npm run proxy\" \"npm run start\""
  },
  "proxy": "http://localhost:8081",
  "keywords": [
    "SharePoint",
    "Classic",
    "template",
    "React",
    "Flux",
    "PnPjs",
    "TypeScript",
    "Sass",
    "WebPack",
    "Jest",
    "Istanbul",
    "PnP PowerShell",
    "proxy",
    "SP Online",
    "SP 2016"
  ],
  "author": "SmukkeD",
  "repository": {
    "type": "Git",
    "url": "https://github.com/smukked/sp-classic"
  },
  "devDependencies": {
    "@proactive-as/pnp.vendor": "^1.2.5",
    "@types/jest": "^23.3.9",
    "@types/node": "^10.12.1",
    "@types/request": "^2.48.0",
    "@types/webpack-env": "^1.13.6",
    "awesome-typescript-loader": "^5.2.1",
    "concurrently": "^4.1.0",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^1.0.1",
    "dateformat": "^3.0.3",
    "del": "^3.0.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "enzyme-to-json": "^3.3.4",
    "fs-extra": "^7.0.0",
    "identity-obj-proxy": "^3.0.0",
    "inline-css": "^2.4.1",
    "jest": "^23.6.0",
    "jest-enzyme": "^7.0.0",
    "jest-trx-results-processor": "^0.0.7",
    "jsdom": "11.10.0",
    "mini-css-extract-plugin": "^0.4.4",
    "node-sass": "^4.9.4",
    "node-style": "^2.0.0",
    "path": "^0.12.7",
    "react-dev-utils": "^6.1.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "sp-rest-proxy": "^2.7.6",
    "style-loader": "^0.23.1",
    "ts-jest": "^23.10.4",
    "tslint": "^5.11.0",
    "tslint-microsoft-contrib": "^5.2.1",
    "typescript": "^3.1.5",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.5",
    "webpack-merge": "^4.1.4",
    "webpack-stream": "^5.1.1"
  },
  "dependencies": {
    "@pnp/pnpjs": "^1.2.3",
    "@pnp/polyfill-ie11": "^1.0.0",
    "@types/enzyme": "^3.1.14",
    "@types/fbemitter": "^2.0.32",
    "@types/flux": "^3.1.8",
    "@types/react": "^16.4.18",
    "@types/react-dom": "^16.0.9",
    "@types/sharepoint": "^2016.1.2",
    "flux": "^3.1.3",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "whatwg-fetch": "^3.0.0"
  }
}
